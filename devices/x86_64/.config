# 编译x64固件:
CONFIG_TARGET_x86=y
CONFIG_TARGET_x86_64=y
CONFIG_TARGET_x86_64_Generic=y

# 设置固件大小:
CONFIG_TARGET_KERNEL_PARTSIZE=32
CONFIG_TARGET_ROOTFS_PARTSIZE=450

# 固件压缩:
CONFIG_TARGET_IMAGES_GZIP=y
CONFIG_TARGET_IMAGES_PAD=y

# 编译常规以及UEFI固件:
CONFIG_GRUB_IMAGES=n
CONFIG_EFI_IMAGES=y

# 编译PVE固件:
CONFIG_QCOW2_IMAGES=n

# 编译VMware镜像
CONFIG_VMDK_IMAGES=n

# 其他需要安装的软件包:
CONFIG_PACKAGE_autocore=y
CONFIG_PACKAGE_automount=y
CONFIG_PACKAGE_autosamba=y
CONFIG_PACKAGE_ipv6-helper=y
CONFIG_PACKAGE_luci-proto-bonding=y


# x86
CONFIG_PACKAGE_qemu-ga=y
CONFIG_PACKAGE_lm-sensors-detect=n
CONFIG_PACKAGE_kmod-bonding=n #lienol没有
CONFIG_PACKAGE_kmod-mmc-spi=n
